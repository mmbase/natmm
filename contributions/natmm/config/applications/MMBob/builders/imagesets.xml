<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<builder extends="object" maintainer="mmbase.org" name="imagesets" version="0">
  <status>active</status>
  <classfile>Dummy</classfile>
  <searchage>9999</searchage>

  <names>
    <singular xml:lang="en">Imageset</singular>
    <singular xml:lang="nl">Afbeeldingverzameling</singular>
    <plural xml:lang="en">Imagesets</plural>
    <plural xml:lang="nl">Afbeeldingverzamelingen</plural>
  </names>

  <descriptions>
    <description xml:lang="en">Set of images</description>
    <description xml:lang="nl">Verzameling afbeeldingen</description>
  </descriptions>

  <fieldlist>
    <field>
      <descriptions>
        <description xml:lang="en">Name of the set</description>
        <description xml:lang="nl">Naam van de verzameling</description>
      </descriptions>
      <gui>
        <guiname xml:lang="en">Name</guiname>
        <guiname xml:lang="nl">Naam</guiname>
        <guitype>string</guitype>
      </gui>
      <editor>
        <positions>
          <input>1</input>
          <list>1</list>
          <search>1</search>
        </positions>
      </editor>
      <db>
        <name xml:lang="en">name</name>
        <type key="false" notnull="true" size="255" state="persistent">STRING</type>
      </db>
    </field>

    <field>
      <descriptions>
        <description xml:lang="en">Description of the set</description>
        <description xml:lang="nl">Omschrijving van de verzameling</description>
      </descriptions>
      <gui>
        <guiname xml:lang="en">Description</guiname>
        <guiname xml:lang="nl">Omschrijving</guiname>
        <guitype>field</guitype>
      </gui>
      <editor>
        <positions>
          <input>2</input>
          <list>2</list>
          <search>2</search>
        </positions>
      </editor>
      <db>
        <name xml:lang="en">description</name>
        <type key="false" notnull="false" size="2048" state="persistent">STRING</type>
      </db>
    </field>
  </fieldlist>
</builder>