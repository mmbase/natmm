<?xml version="1.0"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<!-- mmevents -->
<builder version="0" name="mmevents" maintainer="mmbase.org" extends="object">
    <classfile>MMEvents</classfile>
    <searchage>9999</searchage>
    <names>
        <singular xml:lang="nl">Tijd</singular>
        <singular xml:lang="en">Time</singular>
        <singular xml:lang="us">Time</singular>
        <plural xml:lang="nl">Tijden</plural>
        <plural xml:lang="en">Times</plural>
        <plural xml:lang="us">Times</plural>
    </names>
    <descriptions>
        <description xml:lang="us">Times used to place objects on a fixed time bar (epoch)</description>
    </descriptions>
    <properties/>
    <fieldlist>
        <field>
            <descriptions/>
            <gui>
                <guiname xml:lang="nl">Naam</guiname>
                <guiname xml:lang="us">Name</guiname>
                <guitype>string</guitype>
            </gui>
            <editor>
                <positions>
                    <input>1</input>
                    <list>1</list>
                    <search>1</search>
                </positions>
            </editor>
            <db>
                <name>name</name>
                <type size="64" key="false" state="persistent" notnull="false">STRING</type>
            </db>
        </field>
        <field>
            <descriptions/>
            <gui>
                <guiname xml:lang="nl">Begin</guiname>
                <guiname xml:lang="us">Start</guiname>
                <guitype>eventtime</guitype>
            </gui>
            <editor>
                <positions>
                    <input>2</input>
                    <list>2</list>
                    <search>2</search>
                </positions>
            </editor>
            <db>
                <name>start</name>
                <type key="false" state="persistent" notnull="false">INTEGER</type>
            </db>
        </field>
        <field>
            <descriptions/>
            <gui>
                <guiname xml:lang="nl">Einde</guiname>
                <guiname xml:lang="us">End</guiname>
                <guitype>eventtime</guitype>
            </gui>
            <editor>
                <positions>
                    <input>3</input>
                    <list>3</list>
                    <search>3</search>
                </positions>
            </editor>
            <db>
                <name>stop</name>
                <type key="false" state="persistent" notnull="false">INTEGER</type>
            </db>
        </field>
    </fieldlist>
</builder>
