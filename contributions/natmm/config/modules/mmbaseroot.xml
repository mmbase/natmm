<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//MMBase//DTD module config 1.0//EN" "http://www.mmbase.org/dtd/module_1_0.dtd">
<!--
        The MMBaseRoot module is used to configure the basic properties of mmbase.
-->
<module maintainer="mmbase.org" version="0">
    <status>active</status>
    <classfile>org.mmbase.module.core.MMBase</classfile>
    <properties>
        <property name="machinename">MMBaseserver</property>
        <property name="storagemanagerfactory">org.mmbase.storage.implementation.database.DatabaseStorageManagerFactory</property>
	     <property name="multicasthost"></property>
	     <property name="multicastport">16080</property>
	     <property name="host">127.0.0.1</property>
        <property name="authtype">none</property>
        <property name="dtdbase">http://www.mmbase.org/dtd/</property>
        <property name="basename">@database.tableprefix@</property>
        <property name="language">nl</property>
        <property name="encoding">UTF-8</property>
    </properties>
</module>
