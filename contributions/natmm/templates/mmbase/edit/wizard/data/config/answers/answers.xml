<?xml version="1.0"?>
<!DOCTYPE wizard-schema PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<wizard-schema id="answers">
   <title>opties (bv. 1, 2, 3, of 4 parkeerplaatsen) voor een middel</title>
   <action include="config/answers/create_answers.xml"/>
   <action include="config/answers/delete_answers.xml"/>
   <action include="config/answers/load_answers.xml"/>
   <lists>
      <optionlist include="config/option_lists/checked_aantal.xml" />
   </lists>
   <steps>
      <step form-schema="edit_answers"/>
   </steps>
   <form-schema id="edit_answers" extends="config/answers/edit_answers.xml"/>
</wizard-schema>
