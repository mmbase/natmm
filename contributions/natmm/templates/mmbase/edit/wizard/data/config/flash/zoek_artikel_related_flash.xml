<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="related" destination="flash" minoccurs="0" maxoccurs="1" >
   <title>flash (eerst verwijderen voordat een nieuw item kan worden toegevoegd)</title>
   <item>
      <field ftype="data" name="titel"><prompt>titel</prompt></field>

      <field ftype="startwizard" wizardname="config/flash/flash" />
   </item>

   <command name="search" nodepath="flash" age="-1" fields="titel" orderby="titel">
      <prompt>zoek flash</prompt>
      <search-filter>
         <name>titel bevat</name>
         <search-fields>[titel]</search-fields>
      </search-filter>
   </command>
   <command name="startwizard" inline="true" wizardname="config/flash/flash" objectnumber="new">
       <prompt></prompt>
   </command>
   <action type="create">
      <relation destinationtype="flash" role="related">
      </relation>
   </action>
</list>