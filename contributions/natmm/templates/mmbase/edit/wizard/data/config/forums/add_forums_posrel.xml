<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="posrel" destination="forums"
    minoccurs="0" maxoccurs="1"
    ordertype="number" orderby="field[@name=&apos;pos&apos;]" >
    <title>forum</title>
    <item>
        <title></title>
        <description>forums</description>
        <field name="name" ftype="data">
            <prompt>naam</prompt>
        </field>
    </item>
    <action type="create">
      <relation role="posrel">
        <object type="forums" />
        <field name="pos">1</field>
      </relation>
    </action>
    <command name="search" nodepath="forums" fields="name" orderby="name" age="-1">
        <prompt>zoek forum</prompt>
        <search-filter>
            <name>Naam bevat</name>
            <search-fields>name</search-fields>
        </search-filter>
    </command>
</list>
