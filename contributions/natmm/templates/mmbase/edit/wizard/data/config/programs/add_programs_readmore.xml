<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="readmore" destination="programs"
		minoccurs="0" maxoccurs="*"
		ordertype="number" orderby="field[@name=&apos;pos&apos;]" >
	<title>programma's</title>
	<item>
		<title></title>
		<description>programs</description>
		<field name="title" ftype="data">
			<prompt>titel</prompt>
		</field>
		<field name="subtitle" ftype="data">
			 <prompt>subtitel</prompt>
		</field>
	</item>
	<action type="create">
		<relation role="readmore">
			<object type="programs" />
      </relation>
	</action>
	<command name="search" nodepath="programs" fields="title" orderby="title" age="-1">
    	<prompt>zoek programma</prompt>
		<search-filter>
			<name>titel bevat</name>
			<search-fields>title</search-fields>
		</search-filter>
	</command>
	<command name="startwizard" inline="true" wizardname="config/programs/programs" objectnumber="new"/>
</list>	

