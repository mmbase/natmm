<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="posrel" destination="evenement_type" minoccurs="0" maxoccurs="*"
   orderby="naam" ordertype="string" hidecommand="move-up|move-down">
   <title>getoonde type van activiteiten</title>
   <item>
      <field ftype="data" name="naam"><prompt>naam</prompt></field>
      <!-- <field fdatapath="field[@name=&apos;pos&apos;]" ftype="line"><prompt>standaard kosten</prompt></field> -->
      <!-- field ftype="startwizard" wizardname="config/evenement_type/evenement_type" / -->
   </item>
   <command name="search" nodepath="evenement_type" age="-1" fields="naam" orderby="naam">
      <prompt>zoek activiteitstyp</prompt>
      <search-filter>
         <name>naam bevat</name>
         <search-fields>[naam]</search-fields>
      </search-filter>
   </command>
   <action type="create">
      <relation destinationtype="evenement_type" role="posrel">
      <field name="pos">-1</field>
      </relation>
   </action>
</list>