<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="related" destination="leden" minoccurs="0" maxoccurs="1" >
   <title>leden (eerst verwijderen voordat een nieuw item kan worden toegevoegd)</title>
   <item>
      <field ftype="data" name="naam" />
      <field ftype="data" name="lidnummer" />

      <field ftype="startwizard" wizardname="config/leden/leden" />
   </item>

   <command name="search" nodepath="leden" age="-1" fields="naam,lidnummer" orderby="naam">
      <prompt>zoek lid</prompt>
      <search-filter>
         <name>naam bevat</name>
         <search-fields>[naam]</search-fields>
      </search-filter>
      <search-filter>
         <name>lidnummer bevat</name>
         <search-fields>[lidnummer]</search-fields>
      </search-filter>
   </command>
   <command name="startwizard" inline="true" wizardname="config/leden/leden" objectnumber="new">
       <prompt></prompt>
   </command>

   <action type="create">
      <relation destinationtype="leden" role="related">
      </relation>
   </action>
</list>