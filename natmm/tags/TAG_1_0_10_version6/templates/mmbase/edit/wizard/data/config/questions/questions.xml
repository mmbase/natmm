<?xml version="1.0"?>
<!DOCTYPE wizard-schema PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<wizard-schema id="questions">
   <title>middelen bij een activiteit (bv. parkeerplaatsen)</title>
   <action include="config/questions/create_questions.xml"/>
   <action include="config/questions/delete_questions.xml"/>
   <action include="config/questions/load_questions.xml"/>
   <lists>
      <optionlist include="config/option_lists/question_type.xml" />
      <optionlist include="config/option_lists/ja_nee.xml" />
   </lists>
   <steps>
      <step form-schema="edit_questions"/>
   </steps>
   <form-schema id="edit_questions" extends="config/questions/edit_questions.xml"/>
</wizard-schema>
