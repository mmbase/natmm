<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//MMBase//DTD module config 1.0//EN" "http://www.mmbase.org/dtd/module_1_0.dtd">

<module maintainer="mmbase.org" version="0">

  <status>active</status>
  
  <classfile>org.mmbase.module.database.JDBC</classfile>

  <properties>
  
    <property name="url">@database.url@</property>
    <property name="user">@database.user@</property>
    <property name="password">@database.password@</property>
    <property name="supportclass">org.mmbase.module.database.DatabaseSupportShim</property>
    <property name="database">@database.name@</property>
    <property name="connections">@database.connections@</property>
    <property name="host">@database.host@</property>
    <property name="driver">@database.driver@</property>
    <property name="port">@database.port@</property>
    <property name="queries">256</property>
    <property name="probetime">30</property>
    
  </properties>
  
</module>
