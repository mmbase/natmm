<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="related" destination="feedback" minoccurs="0" maxoccurs="*"
   orderby="sendername" ordertype="string" hidecommand="move-up|move-down">
	<title>feedback</title>
	<item>
		<title>feedback</title>
		<description></description>
		<field name="namesender" ftype="data">
			<prompt>naam</prompt>
		</field>
      <field fdatapath="." ftype="startwizard" inline="true" objectnumber="{object/@number}" wizardname="config/feedback/wizard">
			<prompt></prompt>
		</field>
	 </item>
	<action type="create">
		<relation role="related">
			<object type="feedback" />
		</relation>
	</action>
	<command name="search" nodepath="feedback" fields="namesender,emailsender,topic" orderby="namesender" age="-1">
    	<prompt>zoek feedback</prompt>
		<search-filter>
			<name>naam bevat</name>
			<search-fields>namesender</search-fields>
		</search-filter>
		<search-filter>
			<name>email bevat</name>
			<search-fields>emailsender</search-fields>
		</search-filter>
		<search-filter>
			<name>feedback bevat</name>
			<search-fields>topic</search-fields>
		</search-filter>
	</command>
</list>
