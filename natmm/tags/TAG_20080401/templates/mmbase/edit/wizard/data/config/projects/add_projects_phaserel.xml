<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="phaserel" destination="projects"
    minoccurs="0" maxoccurs="1"
    hidecommand="move-up|move-down">
    <title>begindatum- en einddatum binnen project</title>
    <item>
        <title></title>
        <field name="titel" ftype="data">
            <prompt>Naam</prompt>
        </field>
        <field fdatapath="field[@name=&apos;begindate&apos;]" ftype="date"><prompt>Aanvangsdatum</prompt></field>
        <field fdatapath="field[@name=&apos;enddate&apos;]" ftype="date"><prompt>Einddatum</prompt></field>
    </item>    
    <action type="create">
        <relation role="phaserel">
            <object type="projects" />
        </relation>
    </action>    
    <command name="search" nodepath="phases" fields="titel" orderby="titel" age="-1">
        <prompt>zoek project</prompt>
        <search-filter>
            <name>naam bevat</name>
            <search-fields>titel</search-fields>
        </search-filter>
    </command>
</list>