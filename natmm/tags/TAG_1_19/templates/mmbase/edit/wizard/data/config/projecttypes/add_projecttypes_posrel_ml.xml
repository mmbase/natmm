<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="posrel" destination="projecttypes"
    minoccurs="0" maxoccurs="*"
    ordertype="string" orderby="title"
    hidecommand="move-up|move-down">
    <title>projecttype</title>
    <item>
        <title></title>
        <field name="name" ftype="data">
            <prompt>naam</prompt>
        </field>
        <field fdatapath="." ftype="startwizard" inline="true" objectnumber="{object/@number}" wizardname="config/projecttypes/projecttypes_ml">
             <prompt></prompt>
        </field>
    </item>
    <action type="create">
        <relation role="posrel">
            <object type="projecttypes" />
        </relation>
    </action>
    <command name="search" nodepath="projecttypes" fields="name,subtitle" orderby="name" age="-1">
        <prompt>zoek projecttype</prompt>
        <search-filter>
            <name>naam bevat</name>
            <search-fields>name</search-fields>
        </search-filter>
        <search-filter>
            <name>subtitel bevat</name>
            <search-fields>subtitle</search-fields>
        </search-filter>
    </command>
    <command name="startwizard" inline="true" wizardname="config/projecttypes/projecttypes_ml" objectnumber="new"/>
</list>
