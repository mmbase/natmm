<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="related" destination="providers" minoccurs="1" maxoccurs="*"
   orderby="name" ordertype="string" hidecommand="move-up|move-down">
	<title>aanbieder</title>
	<item>
		<title>aanbieder</title>
		<description></description>
		<field name="naam" ftype="data">
			<prompt>naam</prompt>
		</field>
	 </item>
	<action type="create">
		<relation role="related">
			<object type="providers" />
		</relation>
	</action>
	<command name="search" nodepath="providers" fields="naam,omschrijving" orderby="naam" age="-1">
    	<prompt>zoek aanbieder</prompt>
		<search-filter>
			<name>naam bevat</name>
			<search-fields>naam</search-fields>
		</search-filter>
		<search-filter>
			<name>omschrijving bevat</name>
			<search-fields>omschrijving</search-fields>
		</search-filter>
	</command>
</list>
