<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//MMBase//DTD module config 1.0//EN" "http://www.mmbase.org/dtd/module_1_0.dtd">
<module maintainer="mmbase.org" version="0">
  <status>active</status>
  <classfile>org.mmbase.applications.crontab.modules.JCrontabModule</classfile>
  <properties>  
    <!-- 
    Properties of cronjobs.
    Every property defines one cronjob, and should have a unique name.
    The value of the property has 2 -4 fields (separated by a tab, | or newline),
    first field: when to run (see man crontab) e.g */1 5-23 * * *, 0 22 * * *
    second field: class name of Runnable (or JCronJob implementation if you need init and stop too)
    third field: (optional) a description of the job
    fourth field: (optional) a configuration string (in JCronJobs aquirable by jCronEntry.getConfiguration())
    -->  
    <property name="import">0 2 * * *|nl.mmatch.CSVReader|read the uploaded import files</property>
    <property name="notify">0 3 * * *|nl.leocms.evenementen.EventNotifier|send reminder emails for the CAD</property>
	 <property name="sstats">20 3 * * *|nl.leocms.util.tools.SimpleStats|write statistics from application to object cloud</property>
    <property name="google">40 3 * * *|nl.leocms.connectors.Google.output.sitemap.SiteMapGenerator|writes google sitemap to root dir</property>
	 <property name="update">0 4 * * *|nl.leocms.content.UpdateUnusedElements|update the list of unused elements for each user</property>
	 <property name="documents">40 4 * * *|nl.leocms.util.tools.documents.DirReader|reads the NMIntraConfig.sDocumentsRoot into a document tree</property>
  </properties>
</module>
