<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="related" destination="evenement_distance" minoccurs="0" maxoccurs="*" >
   <title>lengte van deze activiteit (alleen nieuw als lengte nog niet bestaat)</title>
   <item>
      <field ftype="data" name="name"><prompt>naam</prompt></field>
      <!-- field ftype="startwizard" wizardname="config/evenement_distance/evenement_distance" / -->
   </item>
   <command name="search" nodepath="evenement_distance" age="-1" fields="name" orderby="name">
      <prompt>zoek lengte van activiteit</prompt>
      <search-filter>
         <name>naam bevat</name>
         <search-fields>[name]</search-fields>
      </search-filter>
   </command>
   <command name="startwizard" inline="true" wizardname="config/evenement_distance/evenement_distance" objectnumber="new">
       <prompt></prompt>
   </command>
   <action type="create">
      <relation destinationtype="evenement_distance" role="related">
      </relation>
   </action>
</list>