<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="posrel" destination="competencetypes" minoccurs="0" maxoccurs="*"
   orderby="name" ordertype="string" hidecommand="move-up|move-down">
	<title>competentiegroep</title>
	<item>
		<title>competentiegroep</title>
		<description></description>
		<field name="name" ftype="data">
			<prompt>naam</prompt>
		</field>
	 </item>
	<action type="create">
		<relation role="posrel">
			<object type="competencetypes" />
		</relation>
	</action>
	<command name="search" nodepath="competencetypes" fields="name,description" orderby="name" age="-1">
    	<prompt>zoek competentiegroep</prompt>
		<search-filter>
			<name>naam bevat</name>
			<search-fields>name</search-fields>
		</search-filter>
		<search-filter>
			<name>omschrijving bevat</name>
			<search-fields>description</search-fields>
		</search-filter>
	</command>
</list>
